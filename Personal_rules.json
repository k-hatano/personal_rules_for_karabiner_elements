    {
      "description": "Right Command only -> Move focus to the menu bar",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_command"
          },
          "to": [
              {
                  "key_code": "right_command",
                  "lazy": true
              }
          ],
          "to_if_alone": [
            {
              "key_code": "f2",
              "modifiers": [
                "left_control",
                "fn"
              ],
              "repeat": false
            }
          ]
        }
      ]
