{
  "title": "Personal_rules",
  "rules": [
    {
      "description": "Right Command + Right Option + Mouse Button 5 -> Spacebar",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button5",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "spacebar"
            }
          ]
        }
      ]
    },
    {
